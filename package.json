    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "mdbootstrap": "^4.19.1",
    "mdbvue": "^6.7.1",
    "mongodb": "^3.6.1",
    "nuxt": "latest",
    "spdy": "^4.0.2",
    "three": "^0.119.1",
